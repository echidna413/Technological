@model IEnumerable<Tadb.ServerStudio.Models.CatalogModels.FixtureCatalogModel>

@{
    ViewBag.Title = "FixtureCatalogs";
}

<h2>FixtureCatalogs</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.fixture_code)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.fixture_value)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.description)
        </th>
        @*<th></th>*@
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.fixture_code)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.fixture_value)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.description)
            </td>
            @*<td>
                @Html.ActionLink("Edit", "Edit", new { /* id=item.PrimaryKey */ }) |
                @Html.ActionLink("Details", "Details", new { /* id=item.PrimaryKey */ }) |
                @Html.ActionLink("Delete", "Delete", new { /* id=item.PrimaryKey */ })
            </td>*@
        </tr>
    }
</table>

